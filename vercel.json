{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "python3.12"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/admin",
      "dest": "admin.html"
    },
    {
      "src": "/",
      "dest": "index.html"
    }
  ]
}
